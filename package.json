{
  "name": "ReactRelayOfflineNativeExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cli": "node node_modules/react-native/local-cli/cli.js",
    "start": "npm run cli -- start",
    "android": "npm run cli -- run-android",
    "relay:compile": "relay-compiler --src ./src --schema ./schema.graphql --language typescript --artifactDirectory ./src/__generated__",
    "relay:sync": "get-graphql-schema https://etmdb.com/graphql > ./schema.graphql",
    "test": "jest"
  },
  "dependencies": {
    "get-graphql-schema": "^2.1.2",
    "react": "16.8.3",
    "react-native": "0.59.5",
    "react-relay": "^4.0.0",
    "react-relay-offline": "^0.2.0",
    "redux-persist": "^5.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.15",
    "@types/react-native": "^0.57.51",
    "@types/react-relay": "^1.3.14",
    "@types/react-test-renderer": "^16.8.1",
    "@types/redux-persist": "^4.3.1",
    "@types/relay-runtime": "^1.3.12",
    "babel-jest": "^24.7.1",
    "babel-plugin-relay": "^4.0.0",
    "graphql": "^14.2.1",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.8.3",
    "relay-compiler": "^4.0.0",
    "relay-compiler-language-typescript": "^2.5.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
